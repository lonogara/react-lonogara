{
  "name": "lonogara",
  "version": "0.0.7",
  "private": true,
  "main": "dist/cjs.js",
  "jsnext:main": "dist/es.js",
  "module": "dist/es.js",
  "repository": "kthjm/lonogara",
  "author": "kthjm <toxictoxer@gmail.com>",
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "nyc": {
    "require": ["babel-register", "jsdom-global/register"],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "//pretest": "flow",
    "test": "cross-env BABEL_ENV=TEST nyc mocha --recursive",
    "test:require": "mocha test.js",
    "prebuild": "yarn test",
    "build": "cross-env BABEL_ENV=BUILD node rollup.config.js",
    "postbuild": "run-s test:require format",
    "format": "prettier --write **/*.js README.md",
    "prestart": "cross-env BABEL_ENV=WDS node -r babel-register ./wds/preserve.js",
    "start": "cross-env BABEL_ENV=WDS webpack-dev-server --config ./wds/webpack.config.babel.js"
  },
  "dependencies": {
    "atra": "^1.1.2",
    "babel-runtime": "^6.26.0",
    "orph": "^3.0.2",
    "react-centpn": "^0.0.1",
    "react-shut": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "execa": "^0.9.0",
    "flow-bin": "^0.63.1",
    "flow-typed": "^2.2.1",
    "jsdom": "^11.5.1",
    "jsdom-global": "^3.0.2",
    "lonogara-tool": "https://github.com/kthjm/lonogara-tool",
    "mocha": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "power-assert": "^1.4.4",
    "prettier": "^1.10.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "rewire": "^3.0.2",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-flow": "^1.1.1",
    "sinon": "^4.1.5",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  }
}
